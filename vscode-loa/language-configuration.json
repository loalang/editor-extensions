{
  "comments": {
    "blockComment": [
      "/*",
      "*/"
    ],
    "lineComment": "//"
  },
  "autoClosingPairs": [
    [
      "(",
      ")"
    ],
    [
      "[",
      "]"
    ],
    [
      "{",
      "}"
    ]
  ],
  "brackets": [
    [
      "(",
      ")"
    ],
    [
      "[",
      "]"
    ],
    [
      "{",
      "}"
    ]
  ],
  "indentationRules": {
    "increaseIndentPattern": "{",
    "decreaseIndentPattern": "}"
  },
  "surroundingPairs": [
    [
      "(",
      ")"
    ],
    [
      "[",
      "]"
    ],
    [
      "{",
      "}"
    ]
  ]
}