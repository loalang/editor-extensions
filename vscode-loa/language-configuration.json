{
  "comments": {
    "blockComment": ["/*", "*/"],
    "lineComment": "//"
  },
  "autoClosingPairs": [["(", ")"], ["[", "]"], ["{", "}"]],
  "brackets": [["(", ")"], ["[", "]"], ["{", "}"]],
  "indentationRules": {
    "increaseIndentPattern": "{",
    "decreaseIndentPattern": "}",
    "indentNextLinePattern": "(=>$)",
    "unIndentedLinePattern": "let"
  },
  "surroundingPairs": [["(", ")"], ["[", "]"], ["{", "}"]]
}
